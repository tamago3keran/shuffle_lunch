#js-user_index
.main
  %h1 マッチングスコア
  - @matching_scores.each do |matching_score| 
    .matching_score_index
      = matching_score.score
      = matching_score.owner.first_name
      = matching_score.owner.last_name
      = matching_score.partner.first_name
      = matching_score.partner.last_name
  = javascript_include_tag 'packs/user_index.bundle'